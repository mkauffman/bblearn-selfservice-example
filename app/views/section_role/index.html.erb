<div class="contnt">
    <b>Add or Remove Guest Designers</b>
    <br/>
    Click on a section below to add or remove guest designers to/from.
    <br/>
    <table id="add_guest_table" class="output clickable sortable">
        <thead>
            <tr>
                <th id="term">
                    Term
                </th>
                <th id="sect_name">
                    Name
                </th>
            </tr>
        </thead>
        <tbody>
       <% @section_roles.each do |s| %>
       <% link_url = url_for(
        :controller => "section_role",
        :action => 'edit',
        :id => s.section.pk1,
        :enrollment => params[:enrollment])%>
            <tr>
                <td onClick="window.location='<%= link_url %>'">
                    <%= s.section.course_id %>
                </td>
                <td onClick="window.location='<%= link_url %>'">
                    <%= s.section.course_name %>
                </td>
			</tr>
        <% end %>
		</tbody>
	</table>
</div>

